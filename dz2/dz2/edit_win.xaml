<Window x:Class="dz2.edit_win"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dz2"
        mc:Ignorable="d"
        Title="edit" Height="218.333" Width="248.333">
    <Grid>
        <Label x:Name="label" Content="Ime:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="NameTextBox" HorizontalAlignment="Left" Height="23" Margin="47,10,0,0" TextWrapping="Wrap" Text="{Binding Path=ime_vrijednost}" VerticalAlignment="Top" Width="120"/>
        <Label x:Name="label1" Content="Slika:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Label x:Name="FileNameLabel" Height="25" Margin="47,41,10,122" FontSize="9.333"/>
        <Button x:Name="browse" Click="browse_Click" Content="Browse" HorizontalAlignment="Left" Margin="10,72,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="button_ok" Click="button_ok_Click" Content="Save" HorizontalAlignment="Left" Margin="66,160,0,0" VerticalAlignment="Top" Width="76"/>
        <Button x:Name="button_cancle" Click="button_cancle_Click" Content="Cancle" HorizontalAlignment="Left" Margin="161,160,0,0" VerticalAlignment="Top" Width="76"/>

    </Grid>
</Window>
